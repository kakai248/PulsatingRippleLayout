<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PulsatingRippleLayout">
        <attr name="prl_color" format="color" />
        <attr name="prl_strokeWidth" format="dimension"/>
        <attr name="prl_radius" format="dimension"/>
        <attr name="prl_duration" format="integer"/>
        <attr name="prl_rippleAmount" format="integer"/>
        <attr name="prl_scale" format="float"/>
        <attr name="prl_type" format="enum">
            <enum name="fillRipple" value="0"/>
            <enum name="strokeRipple" value="1"/>
        </attr>
    </declare-styleable>
</resources>